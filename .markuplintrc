{
  "extends": [
    "markuplint:html-standard",
    "markuplint:a11y"
  ],
  "nodeRules": [
    {
      "selector": "meta[property]",
      "rules": {
        "invalid-attr": {
          "options": {
            "attrs": {
              "property": {
                "type": "Any"
              },
              "content": {
                "type": "Any"
              }
            }
          }
        },
        "required-attr": false
      }
    },
    {
      "selector": "a[rel]",
      "rules": {
        "invalid-attr": {
          "options": {
            "allowAttrs": ["rel", "me"]
          }
        }
      }
    }
  ]
}
