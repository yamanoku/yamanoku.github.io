{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "moduleResolution": "node",
    "module": "ES2020",
    "target": "ES2020",
    "paths": {
      "~/*": ["src/*"]
    },
    "types": ["vite/client"]
  },
}
